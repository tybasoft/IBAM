}

entity Projet {
	reference String required,
    libelle String required,
    description String,
    dateDebut LocalDate,
    dateFin LocalDate,
    nbrEmploye String,
    budget String,
    adresse String,
    ville String,
    pays String,
	userModif String,
	dateModif LocalDate
}

entity Equipe {
	libelle String required,
	userModif String,
	dateModif LocalDate
}

entity Fonction {
	libelle String required,
