	userModif String,
	dateModif LocalDate
}

entity LigneBonReception {
	quantite String required,
    prixHt String,
	userModif String,
	dateModif LocalDate
}

entity BonCommande {
	datePrevLiv LocalDate,
    remarques String,
    dateCreation LocalDate required,
