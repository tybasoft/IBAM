

entity Materiau {
    libelle String required,
    reference String required,
    poids     String,
    volume    String,
	userModif String,
	dateModif LocalDate
}


entity Tva {
	taux String required,
	userModif String,
	dateModif LocalDate
}

entity Marque {
    libelle String required,
	userModif String,
	dateModif LocalDate
}


entity Unite {
	libelle String required,
	symbole String required,
    description String,
	userModif String,
	dateModif LocalDate
}

/**
 * The Famille entity.
 */
entity Famille {
	libelle String required,
	description String,
	userModif String,
	dateModif LocalDate
}

entity Image {
	titre String required,
	path String required,
	userModif String,
	dateModif LocalDate
}

entity Entreprise {
	entiteJuridique String,
